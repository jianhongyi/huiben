{
    "presets": [
        "@babel/preset-env",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/transform-typescript",
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/plugin-proposal-optional-chaining"
    ]
}